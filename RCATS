MinimizeButton.TextColor3 = Color3.new(1, 1, 1)
MinimizeButton.TextSize = 16

-- Minimizar/Desminimizar la interfaz de usuario
local isMinimized = false
MinimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    if isMinimized then
        Frame:TweenSize(UDim2.new(0, 100, 0, 30), Enum.EasingDirection.InOut, Enum.EasingStyle.Quart, 0.5, true)
    else
        Frame:TweenSize(UDim2.new(0, 840, 0, 480), Enum.EasingDirection.InOut, Enum.EasingStyle.Quart, 0.5, true)
    end
end)

-- Ejecutar el script
local function executeScript(script)
    print("RED Cheats ha ejecutado un script exitosamente.")
    loadstring(script)()
end

ExecuteButton.MouseButton1Click:Connect(function()
    local script = ScriptBox.Text
    if script ~= "" then
        executeScript(script)
    end
end)

-- Borrar el texto
DeleteButton.MouseButton1Click:Connect(function()
    ScriptBox.Text = ""
end)

