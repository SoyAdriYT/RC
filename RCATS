-- Crear la interfaz de usuario
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local PasswordBox = Instance.new("TextBox")

-- Configurar la interfaz de usuario
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(0, 0, 0)
Frame.BorderColor3 = Color3.new(0, 0, 0)
Frame.Position = UDim2.new(0, 0, 0, 0)
Frame.Size = UDim2.new(1, 0, 1, 0)

Title.Parent = Frame
Title.BackgroundColor3 = Color3.new(0, 0, 0)
Title.BorderColor3 = Color3.new(0, 0, 0)
Title.Size = UDim2.new(1, 0, 0, 30)
Title.Font = Enum.Font.SourceSansBold
Title.Text = "Ingrese la clave"
Title.TextColor3 = Color3.new(1, 0, 0)
Title.TextSize = 20

PasswordBox.Parent = Frame
PasswordBox.BackgroundColor3 = Color3.new(0, 0, 0)
PasswordBox.BorderColor3 = Color3.new(1, 0, 0)
PasswordBox.Position = UDim2.new(0.25, 0, 0.5, -15)
PasswordBox.Size = UDim2.new(0.5, 0, 0, 30)
PasswordBox.Font = Enum.Font.SourceSans
PasswordBox.Text = ""
PasswordBox.TextColor3 = Color3.new(1, 0, 0)
PasswordBox.TextSize = 16
PasswordBox.PlaceholderText = "Ingrese la clave..."
PasswordBox.TextWrapped = true
PasswordBox.ClearTextOnFocus = false

-- Función para verificar la clave
