-- Evento para ejecutar el script protegido
ExecuteButton.MouseButton1Click:Connect(function()
    local script = ScriptBox.Text
    if script ~= "" then
        executeProtectedScript(script)
    end
end)

    else
        print("Clave incorrecta")
